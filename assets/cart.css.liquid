#accordion-1  .head h2 {
    color: #040F21;
}
#accordion-1 .arrow{
  color: #563e6e;
  font-size: 60px;
  transition: 0.25s ease;
  opacity: 0.3;
  transform: rotate(-90deg);
}
#accordion-1 .head:hover .arrow{
  opacity: 1;
}
.cart-tab-wrapper { border-top: 1px solid; padding-top: 10px; }
#accordion-1 .arrow-animate{
  transform: rotate(0deg);
  opacity: 1;
}
#accordion-1 .content{
  display: none;
  padding: 10px;
}
  #accordion-1 .head{
  position:relative;  
  }
  #accordion-1 .head:after {
    content: '';
    position: absolute;
    width: 24px;
    height: 24px;
    right: 5px;
    top: 10px;
    background-image: url(https://cdn.shopify.com/s/files/1/0661/1822/2064/files/down-arrow.png?v=1666091652);
    background-size: cover;
    background-repeat: no-repeat;
    transform: rotate(90deg);
}
  #accordion-1 .head.active:after {
    content: "";
    position: absolute;
     width: 24px;
    height: 24px;
    right: 5px;
    top: 10px;
    background-image: url(https://cdn.shopify.com/s/files/1/0661/1822/2064/files/down-arrow.png?v=1666091652);
    background-size: cover;
    background-repeat: no-repeat;
    transform: rotate(360deg);
}

